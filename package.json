{
	"name": "gatsby-headless-blog-with_contentful",
	"private": true,
	"description": "Headless static Gatsby Blog with Contentful",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"build": "env-cmd gatsby build",
		"develop": "env-cmd -f .env gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	},
	"dependencies": {
		"@contentful/rich-text-react-renderer": "^13.4.0",
		"gatsby": "^2.18.12",
		"gatsby-plugin-feed": "^2.3.27",
		"gatsby-plugin-manifest": "^2.2.41",
		"gatsby-plugin-nprogress": "^2.1.19",
		"gatsby-plugin-offline": "^3.0.35",
		"gatsby-plugin-react-helmet": "^3.1.18",
		"gatsby-plugin-sass": "^2.1.26",
		"gatsby-plugin-sharp": "^2.3.10",
		"gatsby-plugin-typography": "^2.3.22",
		"gatsby-remark-images": "^3.1.39",
		"gatsby-remark-relative-images": "^0.2.3",
		"gatsby-source-contentful": "^2.1.73",
		"gatsby-source-filesystem": "^2.1.43",
		"gatsby-transformer-remark": "^2.6.45",
		"node-sass": "^7.0.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-google-recaptcha": "^2.0.1",
		"react-helmet": "^5.2.1",
		"react-hook-form": "^4.9.8",
		"react-typography": "^0.16.19",
		"typography": "^0.16.19",
		"typography-theme-noriega": "^0.16.19"
	},
	"devDependencies": {
		"env-cmd": "^10.0.1",
		"prettier": "^1.19.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
